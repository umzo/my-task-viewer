{
  "compilerOptions": {
    "sourceMap": true,
    "baseUrl": "./",
    "target": "es5",
    "strict": true,
    "module": "commonJs",
    "jsx": "react",
    "lib": [
      "ES5",
      "ES6",
      "DOM"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "strictPropertyInitialization": false,
    "paths": {
      "@controllers/*": ["../../controllers/*"],
      "@gateways/*": ["../../gateways/*"],
      "@presenters/*": ["../../presenters/*"],
      "@domains/*": ["../../domains/*"],
      "~/*": ["./src/*"]
    },
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules"
    ],
  }
}